{
  "name": "content-type-migration",
  "version": "1.0.0",
  "description": "A Kontent.ai custom app to migrate content items between content types",
  "main": "dist/index.html",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:insecure": "vite --host 0.0.0.0 --port 3002",
    "dev:https": "vite --https --host localhost --port 3001",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "cert:generate": "./generate-ssl.sh",
    "cert:install": "./install-cert-macos.sh",
    "setup": "./setup.sh"
  },
  "dependencies": {
    "@kontent-ai/delivery-sdk": "^14.8.0",
    "@kontent-ai/management-sdk": "^5.8.0",
    "@kontent-ai/smart-link": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lucide-react": "^0.263.1",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "keywords": [
    "kontent.ai",
    "custom-app",
    "content-migration",
    "content-types"
  ],
  "author": "Felipe Payan",
  "license": "MIT"
}